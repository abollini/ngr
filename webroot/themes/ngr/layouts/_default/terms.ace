{{ partial "header" . }}

div.row-fluid
  div.col-xs-12
    h1 {{ .Title }}

    ul
      {{ $data := .Data }}
      {{ range $key, $value := .Data.Terms }}
        li
          a href="{{ $data.Plural }}/{{ $key | urlize }}"
            {{ $key }} - {{ len $value }}
      {{ end }}

{{ partial "footer" . }}